            "tag" : {
              "enum" : [ "Protocol Label" ]
            },
            "value" : {
              "type" : "string"
            },
            "unit" : {
              "type" : "string"
            }
          },
          "required" : [ "tag", "value" ]
        },
        "minContains" : 1,
        "maxContains" : 1,
        "recommended" : true
      }, {
        "contains" : {
          "type" : "object",
          "description" : "Conditions at which sample was stored, usually storage temperature, duration and location. In soil context: Explain how and for how long the soil sample was stored before DNA extraction (fresh/frozen/other).",
          "properties" : {
            "tag" : {
              "enum" : [ "sample storage conditions", "storage conditions", "storage conditions (fresh/frozen/other)" ]
            },
            "value" : {
              "type" : "string"
            },
            "unit" : {
              "type" : "string"
            }
          },
          "required" : [ "tag", "value" ]
        },
        "minContains" : 0,
        "maxContains" : 1,
        "recommended" : false
      }, {
        "contains" : {
          "type" : "object",
          "description" : "Origin of stock and raised conditions",
          "properties" : {
            "tag" : {
              "enum" : [ "aquaculture origin" ]
            },
            "value" : {
              "enum" : [ "AOAR - aquacultue origin aquaculture raised", "WOAR - wild origin aquaculture raised", "WOWR - wild origin wild raised" ]
            },
            "unit" : {
              "type" : "string"
            }
          },
          "required" : [ "tag", "value" ]
        },
        "minContains" : 1,
        "maxContains" : 1,
        "recommended" : true
      }, {
        "contains" : {
          "type" : "object",
          "description" : "Total weight of shellfish including shell at the time of sampling. Epifauna and epiphytes to be removed.",
