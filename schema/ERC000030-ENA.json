            "value" : {
              "type" : "string",
              "pattern" : "(0|((0\\.)|([1-9][0-9]*\\.?))[0-9]*)"
            },
            "unit" : {
              "type" : "string"
            }
          },
          "required" : [ "tag", "value" ]
        },
        "minContains" : 0,
        "maxContains" : 1,
        "recommended" : true
      }, {
        "contains" : {
          "type" : "object",
