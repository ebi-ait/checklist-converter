              <TEXT_VALUE>
                <VALUE>Fresh Frozen</VALUE>
              </TEXT_VALUE>
              <TEXT_VALUE>
                <VALUE>Fresh Never Frozen</VALUE>
              </TEXT_VALUE>
            </TEXT_CHOICE_FIELD>
          </FIELD_TYPE>
          <MANDATORY>mandatory</MANDATORY>
          <MULTIPLICITY>single</MULTIPLICITY>
        </FIELD>
        <FIELD>
